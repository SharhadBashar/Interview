import pandas.dataframe as df

# read the dataframe
df = df.open('comments.csv')

data = {}
comments = {}

for i in df:
  if content_type == 'photo' or content_type == 'post' or content_type == 'link':
    data[i[content_id]] = 0
  # its a comment
  else:
    data[i[parent_id]] += 1
    
max_comment_of_one_post = 0
for item in data:
   sum = len(item.value)
   if sum > max_comment_of_one_post: 
      max_comment_of_one_post = sum

# for this ex max_comment_of_one_post = 3
for i in range(max_comment_of_one_post):
  comments[i] = 0
  for item in data:
    if len(item.value) == i:
      comments[i] += 1
    
